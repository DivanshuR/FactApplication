<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.rohatgi.developer.factcheck">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE"/>

    <application
        android:allowBackup="true"
        android:hardwareAccelerated="false"
        android:icon="@drawable/doubt"
        android:label="@string/app_name"
        android:largeHeap="true"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.rohatgi.developer.factcheck.FrontActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.rohatgi.developer.factcheck.TriviaActivity" />
        <activity android:name="com.rohatgi.developer.factcheck.MathActivity" />
        <activity android:name="com.rohatgi.developer.factcheck.YearActivity" />
        <activity android:name="com.rohatgi.developer.factcheck.DateActivity" />
        <activity android:name="com.rohatgi.developer.factcheck.AboutActivity" />
        <activity android:name="com.rohatgi.developer.factcheck.FeedbackActivity" />
        <activity android:name="com.rohatgi.developer.factcheck.MainActivity" />
    </application>

</manifest>